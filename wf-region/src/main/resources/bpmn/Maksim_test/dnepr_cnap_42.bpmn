        <activiti:formProperty id="sCompanyBoss" name="Керівник юридичної особи" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sTrustedPerson" name="Уповноважена особа" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sAdressFiz" name="Адреса проживання фізичної особи" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sAdressUr" name="Юридична адреса (для юридичної особи)" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sAdressParking" name="Адреса стоянки великотоннажного та іншого технологічного транспортного засобу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sType" name="Тип ТТЗ" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sModel" name="Марка, модель ТТЗ" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nYearOfManufacture" name="Рік випуску" type="long" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sCountryManufacture" name="Виробник (держава)" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sIdentificationNumber" name="Ідентифікаційні номери (заводський, шасі/рами)" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sEngineNumber" name="Номер двигуна;(Також у дужках зазначається латинська літера V, знак рівності та об'єм двигуна ТТЗ)" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asDocument" name="Назва завантаженного документу що підтверджує правомірність придбання ТТЗ" type="enum" writable="false">
          <activiti:value id="sEnumDoc1" name="Довідка-рахунок, який видан підприємством-виготовлювачем або іншим суб'єктом господарювання, що реалізує технологічний транспортний засіб"></activiti:value>
          <activiti:value id="sEnumDoc2" name="Акт приймання-передачі, виданий підприємством-виготовлювачем ТТЗ чи його вузлів і агрегатів"></activiti:value>
          <activiti:value id="sEnumDoc3" name="Договір купівлі-продажу, міни, дарування, лізингу, позички, застави, біржової угоди, договір про спільне володіння, користування і розпорядження майном, що перебуває у спільній частковій власності"></activiti:value>
          <activiti:value id="sEnumDoc4" name="Документ, що підтверджує реалізацію технологічного транспортного засобу на аукціоні"></activiti:value>
          <activiti:value id="sEnumDoc5" name="Договір оренди майна із зазначенням у ньому домовленості між сторонами щодо реєстрації ТТЗ за орендарем та доручення на право керування і використання цього транспортного засобу"></activiti:value>
          <activiti:value id="sEnumDoc6" name="Вантажна митна декларація на технологічний транспортний засіб, що ввозиться, або посвідченням митного органу про реєстрацію ввезених на митну територію України ТТЗ, вузлів і агрегатів"></activiti:value>
          <activiti:value id="sEnumDoc7" name="Рішення уповноваженого органу про відчуження ТТЗ"></activiti:value>
          <activiti:value id="sEnumDoc8" name="Виконавчий напис нотаріуса про витребування від боржника майна"></activiti:value>
          <activiti:value id="sEnumDoc9" name="Рішення суду про визнання права власності на ТТЗ"></activiti:value>
          <activiti:value id="sEnumDoc10" name="Договір про спільне володіння, користування у разі отримання ТТЗ кількома громадянами в рахунок майнових паїв"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sExtensionNumber" name="Реєстраційний номер ЦНАП" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAdminFio" name="П.І.Б. адміністратора ЦНАП, який прийняв заяву" type="string"></activiti:formProperty>
