    </sequenceFlow>
    <sequenceFlow id="flow9" name="ок" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${asDecision1=='accept1'}]]></conditionExpression>
    </sequenceFlow>
