        <activiti:formProperty id="visitDate1" name="Дату та час проведення обряду" type="queueData"></activiti:formProperty>
        <activiti:formProperty id="sSourceType" name="Джерело надходження звернення" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="isOk" name="Чи відбулась реєстрація розірвання шлюбу?" type="enum" required="true">
          <activiti:value id="accept" name="Реєстрація розірвання шлюбу відбулась"></activiti:value>
          <activiti:value id="reject" name="Реєстрація розірвання шлюбу скасована. Звільнити місця в електронній черзі."></activiti:value>
        </activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow7" sourceRef="usertask2" targetRef="exclusivegateway2"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow8" sourceRef="exclusivegateway2" targetRef="endevent2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isOk == 'accept'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow9" sourceRef="exclusivegateway2" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isOk == 'reject'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask3" name="Звільнення черги" activiti:delegateExpression="#{releaseTicketsOfQueue}"></serviceTask>
    <sequenceFlow id="flow10" sourceRef="servicetask3" targetRef="endevent3"></sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow14" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${bReferent == 'false'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask4" name="сповіщення співробітника" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
