    <scriptTask id="scripttask1" name="Edit Email" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var per4 = 'substance'
var per5 = 'substance_2'
var per6 = 'substance_3'
var per7 ='term'
if (execution.getVariable('vibor_service') == 'issue'){
per4 = 'видати'
per5 = 'Ваш дозвіл на всі види досліджень та експериментальні роботи з мікроорганізмами ІІІ-IV групи патогенності (небезпеки) або матеріалом підозрілим на їх вміст - готовий'
per6 = 'видачі'
per7 = 'десяти робочих днів'
}
if (execution.getVariable('vibor_service') == 'restructuring'){
per4 = 'переоформити'
per5 = 'Ваш переоформлений дозвіл на всі види досліджень та експериментальні роботи з мікроорганізмами ІІІ-IV групи патогенності (небезпеки) або матеріалом підозрілим на їх вміст - готовий'
per6 = 'переоформлення'
per7 = 'двох робочих днів'
}
if (execution.getVariable('vibor_service') == 'duplicate'){
per4 = 'видати дублікат'
per5 = 'Ваш дублікат дозволу на всі види досліджень та експериментальні роботи з мікроорганізмами ІІІ-IV групи патогенності (небезпеки) або матеріалом підозрілим на їх вміст - готовий'
per6 = 'видачі дублікату'
per7 = 'двох робочих днів'
}
if (execution.getVariable('vibor_service') == 'cansel'){
per4 = 'анулювати'
per5 = 'Рішення про анулювання документа дозвільного характеру - готове'
per6 = 'анулювання'
per7 = 'п`яти робочих днів'
}
execution.setVariable('substance', per4)
execution.setVariable('substance_2', per5)
execution.setVariable('substance_3', per6)
execution.setVariable('term', per7)</script>
    </scriptTask>
    <scriptTask id="scripttask2" name="List Documents" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var per1 = ' '
var per2 = ' '
var per3 = ' '
var res = 'res'
if (execution.getVariable('vibor_service') == 'issue') {
per1 = '&lt;li&gt;Клопотання керівника установи (підприємства) на проведення діагностичних досліджень або експериментальних робіт.&lt;/li&gt;'
per2 = "&lt;li&gt;Акти комісії з контролю біологічної безпеки установи та територіальної режимної комісії щодо дотримання та виконання вимог техніки безпеки, протиепідемічного режиму, особистої гігієни, тощо з додаванням план-схеми лабораторії з зазначенням руху заразного або підозрілого на його вміст матеріалу (патогенних мікроорганізмів) в процесі дослідження та переліку мікроорганізмів, з якими проводяться роботи в лабораторії, відомостей про підготовку персоналу і щеплення, наявності та порядок використання засобів індивідуального захисту.&lt;/li&gt;"
per3 = "&lt;li&gt;Висновок головного державного санітарного лікаря території щодо наявності умов для проведення даного виду робіт та можливості видачі дозволу на роботу.&lt;/li&gt;"
res = '&lt;ol&gt;'+per1+per2+per3+'&lt;/ol&gt;'
}
if (execution.getVariable('vibor_service') == 'restructuring') {
per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає переоформленню.&lt;/li&gt;'
per2 = '&lt;li&gt;Документ, який засвідчує настання підстави для переоформлення документу дозвільного характеру.&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+per2+'&lt;/ol&gt;'
}
if (execution.getVariable('vibor_service') == 'duplicate') {
per1 = '&lt;li&gt;Бланк документу дозвільного характеру, який непридатний для використання.&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
}
if (execution.getVariable('vibor_service') == 'cansel') {
per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає анулюванню.&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
}
execution.setVariable('list_documents', res)</script>
    </scriptTask>
