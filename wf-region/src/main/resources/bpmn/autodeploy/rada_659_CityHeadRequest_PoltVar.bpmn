    <scriptTask id="scripttask1" name="СКР Адреса" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var Vul = execution.getVariable('sRegistrationVul')
var Bud = execution.getVariable('sRegistrationBud')
var Korp = execution.getVariable('lRegistrationKorp')
var Kvart = execution.getVariable('lRegistrationKvart')
var Adres = ' ' 
  
if(execution.getVariable('asBudType') == 'sBagkvatur')
  {
  if(execution.getVariable('asKorp') == 'yes'){Adres='м.Дніпро, '+Vul+', б. '+Bud+', корп. '+Korp+', кв. '+Kvart}
  if(execution.getVariable('asKorp') == 'no'){Adres='м.Дніпро, '+Vul+', б. '+Bud+', кв. '+Kvart}
  }
if(execution.getVariable('asBudType') == 'sPrivSector')
  {
  if(execution.getVariable('asKvart') == 'yes'){Adres='м.Дніпро, '+Vul+', б. '+Bud+', кв. '+Kvart}
  if(execution.getVariable('asKvart') == 'no'){Adres='м.Дніпро, '+Vul+', б. '+Bud}
  }  
execution.setVariable('ScriptAdress', Adres)
      </script>
