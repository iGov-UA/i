	}
	if (execution.getVariable('nFile_Dovirenist') != null  &amp;&amp; execution.getVariable('nFile_Dovirenist')!= ' ')
	{
		per2 = per2 + 1
		per1 = per1 + sStartTag + per2.toString() + '. Копія довіреності на представника. &lt;br&gt;' + sEndTag  
	}

	execution.setVariable('sAddDoc', per1) 
	
