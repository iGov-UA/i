    <scriptTask id="scripttask1" name="Передача значений (javascript)" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
      var src=execution.getVariable('date_of_visit')
var year=src.substr(0,4)
var month=src.substr(5,2)
var day=src.substr(8,2)

minpay='13,60'
maxpay='40,80'

if (year=='2017') {
	minpay='27,20'
    maxpay='81,60'

	minpay='13,60'
    maxpay='40,80'    
}

execution.setVariable('sMinPay', minpay)
execution.setVariable('sMaxPay', maxpay)
	  </script>
    </scriptTask>
    <scriptTask id="scripttask2" name="Script Task (JavaScript)" scriptFormat="javascript" activiti:autoStoreVariables="false">
