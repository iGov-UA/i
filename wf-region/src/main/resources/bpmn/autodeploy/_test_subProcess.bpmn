            <activiti:expression><![CDATA[${sTable2}]]></activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow23" sourceRef="scripttask2" targetRef="usertask4"></sequenceFlow>
    <endEvent id="endevent4" name="End"></endEvent>
