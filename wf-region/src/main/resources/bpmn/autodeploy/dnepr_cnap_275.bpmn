    <serviceTask id="servicetask4" name="повідомлення про рішення" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[test_noreply@igov.org.ua]]></activiti:string>
