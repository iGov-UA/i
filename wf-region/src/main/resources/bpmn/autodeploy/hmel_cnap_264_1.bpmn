  <process id="hmel_cnap_264_1" name="Дублікат посвідчення багатодітної сім'ї та дитини з багатодітної сім'ї у разі його втрати" isExecutable="true">
    <documentation>https://github.com/e-government-ua/iBP/issues/687</documentation>
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть свій район або місто" type="select" default="0" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sAdres" name="Адреса реєстрації громадянина" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Вкажіть контактний номер телефону" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sPib" name="ПІБ особи, посвідчення якої втрачено" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sDocNewspaper" name="Газета з розміщеним оголошенням у ЗМІ про те, щоб втрачене посвідчення вважати недійсним" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="sDocFamily" name="Довідка про склад сім’ї" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="sNameOrgan" name="назва адміністративного органу; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sWorkTime" name="графік роботи; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sPhoneOrgan" name="телефон для довідок; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAddress" name="адреса адміністративного органу; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sMailClerk" name="пошта посадовця; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sShapka494" name="шапка у принтформу; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sListDocumentsPrint" name="перелік у принтформу; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sListDocumentsLetter" name="перелік у лист; ;bVisible=false" type="string"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <userTask id="usertask2" name="Запрошення громадян для надання оригіналів" activiti:candidateGroups="${sID_Public_SubjectOrganJoin}">
      <extensionElements>
        <activiti:formProperty id="sNameOrgan" name="Район" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="clfio" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sAdres" name="Адреса реєстрації громадянина; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний номер телефону" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sPib" name="ПІБ особи, посвідчення якої втрачено; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="decision_2" name="прийняте рішення" type="enum" required="true">
          <activiti:value id="accept" name="позитивне рішення - запросити громадянина для отримання дублікату"></activiti:value>
          <activiti:value id="reject" name="відхилити звернення"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sDateVisit2" name="Вкажіть дату і час - коли прийти громадянину за дублікатом, у разі потреби - додаткові відомості щодо візиту" type="string"></activiti:formProperty>
        <activiti:formProperty id="comment_2" name="У разі відхилення внесіть, будь ласка, причину" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="markers20" name="якщо відмова" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_20&quot;:{&quot;aField_ID&quot;:[&quot;comment_2&quot;],&quot;asID_Field&quot;:{&quot;sReason&quot;:&quot;decision_2&quot;},&quot;sCondition&quot;:&quot;[sReason] == 'reject'||[sReason] == 'відхилити звернення'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers21" name="якщо погоджено" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_21&quot;:{&quot;aField_ID&quot;:[&quot;sDateVisit2&quot;],&quot;asID_Field&quot;:{&quot;sReason&quot;:&quot;decision_2&quot;},&quot;sCondition&quot;:&quot;[sReason] == 'accept'||[sReason] == 'позитивне рішення - запросити громадянина для отримання дублікату'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="sListDocumentsPrint" name="перелік у принтформу; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sShapka494" name="шапка у принтформу; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="PrintForm_1" name="File label;File title;pattern/print/khmelnitsky/hmel_cnap_264_1_print.html" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sBody_1" name="[pattern/print/khmelnitsky/hmel_cnap_264_1_print.html]; ;bVisible=false" type="string" default="заява загальна" writable="false"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression>${sDocNewspaper},${sDocFamily}</activiti:expression>
          </activiti:field>
        </activiti:taskListener>
