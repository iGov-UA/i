    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var per1 = "&lt;li&gt;Роздруковану заяву, яку ви подавали в електронному вигляді (знаходиться у додатку до листа), засвідчену особистим підписом (підписами).&lt;/li&gt;"
var per2 = " "
var per3 = " "
var per4 = " "
var per5 = " "
var per6 = " "
var res = 'res'
var res2 = 'res2'
var per7 = " "
var per8 = " "
if(execution.getVariable('sDocMarriage') != null){
per2 = '&lt;li&gt;Копія свідоцтва про шлюб.&lt;/li&gt;'
}
if(execution.getVariable('sDocPassportFather') != null){
per3 = "&lt;li&gt;Копія паспорту батька.&lt;/li&gt;"
}
if(execution.getVariable('sDocPassportMother') != null){
per4 = "&lt;li&gt;Копія паспорту матері.&lt;/li&gt;"
}
if(execution.getVariable('sDocMedical') != null){
per5 = '&lt;li&gt;Медичне свідоцтво про народження.&lt;/li&gt;'
}
if(execution.getVariable('sDocPregnancy') != null){
per6 = "&lt;li&gt;Довідка про вагітність (видається закладами охорони здоров’я).&lt;/li&gt;"
}
if(execution.getVariable('saSexChild') == 'woman'){
per7 = "що народилася"
}
if(execution.getVariable('saSexChild') == 'man'){
per7 = "що народився"
}
if(execution.getVariable('saSexChild') == 'woman'){
per8 = "її"
}
if(execution.getVariable('saSexChild') == 'man'){
per8 = "його"
}
res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+'&lt;/ol&gt;'
res2 = '&lt;ol&gt;'+per2+per3+per4+per5+per6+'&lt;/ol&gt;'
execution.setVariable('sListDocumentsLetter', res)
execution.setVariable('sListDocumentsPrint', res2)
execution.setVariable('sSexChildPrint', per7)
execution.setVariable('sSexChildPrint2', per8)
</script>
    </scriptTask>
