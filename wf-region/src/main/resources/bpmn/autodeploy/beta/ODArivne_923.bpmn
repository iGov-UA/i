          <activiti:value id="accept" name="Заяву прийнято, видати сертифікат племінних ресурсів"></activiti:value>
          <activiti:value id="reject" name="Заявка оформлена НЕ вірно, відмова"></activiti:value>
        </activiti:formProperty>
